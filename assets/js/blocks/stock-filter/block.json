{
	"name": "woocommerce/stock-filter",
	"version": "1.0.0",
	"title": "Filter by Stock",
	"description": "Allow customers to filter the grid by products stock status.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce" ],
	"supports": {
		"html": false,
		"multiple": false,
		"color": {
		  "link": true,
		  "__experimentalDefaultControls": {
			"text": true
		  }
		}
	},
	"example": {
		"attributes": {
			"isPreview": true
		}
	},
	"attributes": {
		"className": {
			"type": "string",
			"default": ""
		},
		"headingLevel": {
			"type": "number",
			"default": 3
		},
		"showCounts": {
			"type": "boolean",
			"default": true
		},
		"showFilterButton": {
			"type": "boolean",
			"default": false
		},
		"isPreview": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "woo-gutenberg-products-block",
	"apiVersion": 2,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
