{
	"name": "woocommerce/mini-cart-title-your-cart-block",
	"version": "1.0.0",
	"title": "Mini Cart Title Your Cart",
	"description": "Block that displays the 'Your cart' part of the Mini Cart Title block.",
	"category": "woocommerce",
	"supports": {
		"align": false,
		"html": false,
		"multiple": false,
		"reusable": false,
		"inserter": true,
		"lock": false,
		"color": {
			"text": true,
			"background": true
		},
		"typography": {
			"fontSize": true
		}
	},
	"attributes": {
		"label": {
			"type": "string"
		},
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": true
			}
		}
	},
	"parent": [ "woocommerce/mini-cart-title-block" ],
	"textdomain": "woo-gutenberg-products-block",
	"apiVersion": 2,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
