{
  "name": "woocommerce/woocommerce-gutenberg-products-block",
  "description": "Feature plugin for the WooCommerce Gutenberg Products block",
  "homepage": "https://woocommerce.com/",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "composer/installers": "1.6.0",
    "automattic/jetpack-autoloader": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.14",
    "woocommerce/woocommerce-sniffs": "0.0.6"
  },
  "autoload": {
    "psr-4": {
      "Automattic\\WooCommerce\\Blocks\\": "src/",
      "Automattic\\WooCommerce\\Blocks\\Tests\\": "tests/php/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload --no-dev"
    ],
    "post-update-cmd": [
      "composer dump-autoload --no-dev"
    ],
    "phpcs": [
      "phpcs --extensions=php -s -p"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  },
  "extra": {
    "scripts-description": {
      "phpcs": "Analyze code against the WordPress coding standards with PHP_CodeSniffer",
      "phpcbf": "Fix coding standards warnings/errors automatically with PHP Code Beautifier"
    }
  }
}
